@using WeMoveU.LandingPage.Web.Models
@model  DadosCadastro
@using System.Web.Optimization
@{
    ViewBag.Title = "Index";
}


<!-- Top menu -->
<nav class="navbar navbar-inverse" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#top-navbar-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="index.html"></a>
        </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="top-navbar-1">
            <ul class="nav navbar-nav navbar-right">
                <li><a class="scroll-link" href="#features">Benefícios</a></li>
                <li><a class="scroll-link" href="#how-it-works">Produto</a></li>
                <li><a class="scroll-link" href="#testimonials">Contato</a></li>

            </ul>
        </div>
    </div>
</nav>

<!-- Top content -->
<div class="top-content">
    <div class="container">

        <div class="row">
            <div class="col-sm-8 col-sm-offset-2 text">
                <h1 class="wow fadeInLeftBig">ADQUIRA SEU RELATÓRIO AGORA DE R$99,90 POR R$49,90</h1>
                <div class="description wow fadeInLeftBig">
                    <p>
                        Melhore o seu site nas redes de busca.
                        Mais de 1 milhão e meio de pessoas buscam imobiliárias todo mês no Google
                    </p>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-sm-6 col-sm-offset-3 r-form-1-box wow fadeInUp">
                <div class="r-form-1-top">
                    <div class="r-form-1-top-left">
                        <h3>Obter Relatório</h3>
                        <p>Faça seu cadastro:</p>
                    </div>
                    <div class="r-form-1-top-right">
                        <i class="fa fa-pencil"></i>
                    </div>
                </div>
                <div class="r-form-1-bottom">
                    @using (Html.BeginForm("Index", "Home", FormMethod.Post))
                    {
                      <!--ValidationSummary: Exibe apenas os erros identificados com false, com true apresenta qualquer erro-->
                        @Html.ValidationSummary(false)
    <!--Cria Token de sessão-->
                        @Html.AntiForgeryToken()
                        <div class="form-group">
                            <label class="sr-only" for="r-form-1-first-name">First name</label>
                            <input type="text" name="Nome" placeholder="Nome..." class="r-form-1-first-name form-control" id="Nome">
                        </div>
                        <div class="form-group">
                            <label class="sr-only" for="r-form-1-last-name">Last name</label>
                            <input type="text" name="Sobrenome" placeholder="Sobrenome..." class="r-form-1-last-name form-control" id="Sobrenome">
                        </div>
                        <div class="form-group">
                            <label class="sr-only" for="r-form-1-email">Email</label>
                            <input type="text" name="Email" placeholder="Email..." class="r-form-1-email form-control" id="Email">
                        </div>
                        <div class="form-group">
                            <label class="sr-only" for="r-form-1-email">Website</label>
                            <input type="text" name="Website" placeholder="Website..." class="r-form-1-Website form-control" id="Website" >
                        </div>
                        <div class="form-group">
                            <label class="sr-only" for="r-form-1-about-yourself">About yourself</label>
                            <textarea name="r-form-1-about-yourself" placeholder="Mensagem"
                                      class="r-form-1-about-yourself form-control" id="Mensagem"></textarea>
                        </div>
                        <button type="submit" class="btn">Enviar!</button>
                    
                    }
                </div>
            </div>
        </div>

    </div>
</div>

<!-- Features -->
<div class="features-container section-container">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 features section-description wow fadeIn">
                <h2>OBTENHA AGORA UM RELATÓRIO PERSONALIZADO SOBRE A SITUAÇÃO DO SEU NEGÓCIO</h2>
                <div class="divider-1 wow fadeInUp">. . . . . . . . . .</div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4 features-box wow fadeInUp">
                <div class="features-box-icon"><i class="fa fa-thumbs-up"></i></div>
                <h3>Mais Clientes</h3>
                <p>Aumente o número de clientes somente por estar em primeiro nas buscas!</p>
            </div>
            <div class="col-sm-4 features-box wow fadeInDown">
                <div class="features-box-icon"><i class="fa fa-cog"></i></div>
                <h3>Baixo Custo</h3>
                <p>Baixo custo de investimento e manutenção.</p>
            </div>
            <div class="col-sm-4 features-box wow fadeInUp">
                <div class="features-box-icon"><i class="fa fa-commenting"></i></div>
                <h3>Retorno Garantido</h3>
                <p>Melhor retorno sobre o seu investimento possível!</p>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 section-bottom-button wow fadeInUp">
                <a class="btn btn-link-1 scroll-link" href="#top-content">Obtenha Agora!<i class="fa fa-angle-right"></i></a>
            </div>
        </div>
    </div>
</div>


<!-- FAQ -->
<div class="faq-container section-container">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 faq section-description wow fadeIn">
                <h2>Confira seus benefícios</h2>
                <div class="divider-1 wow fadeInUp">. . . . . . . . . .</div>
               
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4 col-sm-offset-1 faq-box wow fadeInUp">
                <div class="faq-number">1</div>
                <h3>Posicionamento no Google</h3>

            </div>
            <div class="col-sm-4 col-sm-offset-2 faq-box wow fadeInDown">
                <div class="faq-number">2</div>
                <h3>Posicionamento dos Concorrentes</h3>
            </div>
            <div class="row">
                <div class="col-sm-4 col-sm-offset-1 faq-box wow fadeInUp">
                    <div class="faq-number">3</div>
                    <h3>Dicas para melhor posicionamento</h3>

                </div>
                <div class="col-sm-4 col-sm-offset-2 faq-box wow fadeInDown">
                    <div class="faq-number">4</div>
                    <h3>Previsão de vendas ao alcançar a posição numero 1</h3>

                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 section-bottom-button wow fadeInUp">
                    <a class="btn btn-link-1 scroll-link" href="#top-content">Obter Agora! <i class="fa fa-angle-right"></i></a>
                </div>
            </div>
        </div>
    </div>

   
    <center>
        <div class="col-sm-4 footer-social wow fadeInUp">
            <h3>We're Social</h3>
            <p>
                <a href="#"><i class="fa fa-facebook"></i></a>
                <a href="#"><i class="fa fa-twitter"></i></a>
                <a href="#"><i class="fa fa-google-plus"></i></a>
                <a href="#"><i class="fa fa-instagram"></i></a>
                <a href="#"><i class="fa fa-pinterest"></i></a>
            </p>
        </div>
</div>

        </footer></center>


<!-- Javascript -->
<script src="~/Content/assets/js/jquery-1.11.1.min.js"></script>
<script src="~/Content/assets/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Content/assets/js/jquery.backstretch.min.js"></script>
<script src="~/Content/assets/js/wow.min.js"></script>
<script src="~/Content/assets/js/retina-1.1.0.min.js"></script>
